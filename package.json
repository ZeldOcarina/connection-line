{
	"name": "connection-line",
	"version": "1.1.0",
	"description": "A new website for Cl Connection Line SAGL",
	"main": "app.js",
	"scripts": {
		"start": "node server.js",
		"start:dev": "nodemon server.js",
		"start:testProd": "NODE_ENV=production nodemon server.js",
		"prod": "node server.js NODE_ENV=production",
		"watch:js": "webpack --config webpack.dev.js",
		"build:js": "webpack --config webpack.prod.js",
		"sass": "node-sass src/scss/main.scss public/css/app.min.css -w",
		"compile:sass": "node-sass src/scss/main.scss public/css/app.comp.css",
		"prefix:css": "postcss public/css/app.comp.css -o public/css/style.prefix.css",
		"compress:css": "node-sass public/css/style.prefix.css public/css/app.min.css --output-style compressed",
		"build:css": "npm-run-all compile:sass prefix:css compress:css",
		"build": "npm-run-all build:js compile:sass prefix:css compress:css"
	},
	"repository": {
		"type": "git",
		"url": ".git"
	},
	"author": "Mattia Rasulo",
	"license": "ISC",
	"engines": {
		"node": "10.16.0"
	},
	"dependencies": {
		"@babel/polyfill": "^7.4.4",
		"aws-sdk": "^2.515.0",
		"axios": "^0.19.0",
		"bcryptjs": "^2.4.3",
		"body-parser": "^1.19.0",
		"busboy": "^0.3.1",
		"cookieconsent": "^3.1.1",
		"cors": "^2.8.5",
		"counterup": "^1.0.2",
		"countup.js": "^2.0.4",
		"dotenv": "^8.0.0",
		"ejs": "^2.6.1",
		"express": "^4.16.4",
		"express-rate-limit": "^5.0.0",
		"express-recaptcha": "^5.0.0",
		"express-sanitizer": "^1.0.5",
		"helmet": "^3.18.0",
		"jquery": "^3.4.1",
		"jquery-scrollify": "^1.0.20",
		"js-cookie": "^2.2.1",
		"jsonwebtoken": "^8.5.1",
		"mongoose": "^5.5.7",
		"mongoose-slug-updater": "^3.1.4",
		"multer": "^1.4.2",
		"multer-s3": "^2.9.0",
		"nodemailer": "^6.2.1",
		"owl.carousel": "^2.3.4",
